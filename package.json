{
  "name": "debuglog",
  "version": "0.0.1",
  "description": "Debuglog for DeltaChat",
  "keywords": [
    "webxdc",
    "deltachat"
  ],
  "contributors": [
    {
      "name": "Martijn Faassen",
      "email": "faassen@startifact.com",
      "url": "http://blog.startifact.com"
    }
  ],
  "files": [
    "./dist"
  ],
  "scripts": {
    "dev": "vite",
    "webxdc-dev": "concurrently \"npm run dev\" \"webxdc-dev run http://localhost:3000\"",
    "build": "vite build",
    "webxdc-build": "npm run build && rimraf ./debuglog.xdc && cd dist && bestzip --level 9 ../debuglog.xdc *",
    "webxdc-dev-xdc": "npm run webxdc-build && webxdc-dev run debuglog.xdc",
    "serve": "vite preview",
    "test": "jest",
    "typecheck": "tsc --project tsconfig.build.json",
    "version": "version-changelog CHANGELOG.md && git add CHANGELOG.md",
    "prepublishOnly": "npm run build"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-typescript": "^7.16.7",
    "@tsconfig/recommended": "^1.0.1",
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.21",
    "babel-jest": "^28.1.2",
    "babel-preset-solid": "^1.0.0",
    "bestzip": "^2.2.1",
    "concurrently": "^7.2.2",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.4.0",
    "fake-indexeddb": "^4.0.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "jsdom": "^19.0.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "solid-jest": "^0.2.0",
    "typescript": "^4.6.4",
    "version-changelog": "^3.1.1",
    "vite": "^2.9.8",
    "vite-plugin-solid": "^2.2.6",
    "webxdc-dev": "^0.15.1"
  },
  "dependencies": {
    "@tanstack/virtual-core": "^3.0.0-alpha.1",
    "dexie": "^3.2.2",
    "fg-select-css": "^3.2.0",
    "solid-dexie": "^0.0.5",
    "solid-js": "^1.4.6",
    "the-new-css-reset": "^1.7.2"
  }
}
